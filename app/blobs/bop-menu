    parse: function (s) {
      var ary = s.split(':')
      if(ary.length < 2) return null
      if('unix' !== ary.shift()) return null
      return {
        name: '',
        path: ary.shift()
      }
    },
    stringify: function (_scope) {
      if(scope !== _scope) return null
      return ['unix', socket].join(':')
    }
  }
}

